<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminUpdateProductActivity">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_arrow_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:text="Update Product"
        android:textColor="@color/pink_primary"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:text="@string/proid"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/edtidproduct"
        app:layout_constraintEnd_toStartOf="@+id/textView24"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView17"
        app:layout_constraintVertical_bias="0.755" />

    <EditText
        android:id="@+id/edtidproduct"
        android:layout_width="356dp"
        android:layout_height="49dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:hint="@string/enidpro"
        android:inputType="text"
        android:paddingLeft="16dp"
        android:text=""
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/namepro"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editnameproduct"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/edtidproduct"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editnameproduct"
        android:layout_width="356dp"
        android:layout_height="49dp"
        android:layout_marginTop="156dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:inputType="text"
        android:paddingLeft="16dp"
        android:text="@string/ennampro"
        android:textColor="@color/grey"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView17" />

    <TextView
        android:id="@+id/eeeeee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pr"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/edtprice"
        app:layout_constraintStart_toStartOf="@+id/textView29"
        app:layout_constraintTop_toBottomOf="@+id/editnameproduct" />

    <EditText
        android:id="@+id/edtprice"
        android:layout_width="169dp"
        android:layout_height="48dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:text="@string/enpr"
        android:textColor="@color/grey"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editnameproduct" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="8dp"
        android:text="@string/st"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/eeeeee"
        app:layout_constraintTop_toBottomOf="@+id/editnameproduct" />

    <EditText
        android:id="@+id/edtstock"
        android:layout_width="168dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:text="@string/enst"
        android:textColor="@color/grey"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/edtprice"
        app:layout_constraintStart_toEndOf="@+id/edtprice" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/des"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/eeeeee"
        app:layout_constraintTop_toBottomOf="@+id/edtprice" />

    <EditText
        android:id="@+id/edt_des"
        android:layout_width="349dp"
        android:layout_height="94dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:paddingTop="1dp"
        android:text="@string/endes"
        android:textColor="@color/grey"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/imglink"
        app:layout_constraintStart_toStartOf="@+id/textView34"
        app:layout_constraintTop_toBottomOf="@+id/edt_des" />

    <EditText
        android:id="@+id/img_link"
        android:layout_width="348dp"
        android:layout_height="59dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/rounded_edittext_black_stroke"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:paddingTop="1dp"
        android:text="@string/enimg"
        android:textColor="@color/grey"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnupdate"
        android:layout_width="356dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/rounded_pink_button_background"
        android:text="@string/uppro"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_link" />

</androidx.constraintlayout.widget.ConstraintLayout>